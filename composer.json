{
    "name": "mklj/rudolf",
    "type": "project",
    "description": "Rudolf is simple CMS written in PHP. That's all.",
    "authors": [
        {
            "name": "MikoÅ‚aj Pich",
            "email": "m.pich@outlook.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "psr/log": "^1.0",
        "phpixie/image": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {
            "Rudolf\\Framework\\": "app/framework/",
            "Rudolf\\Component\\": "app/component/",
            "Rudolf\\Modules\\": "app/module/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rudolf\\Tests\\Component\\": "app/tests/component/",
            "Rudolf\\Tests\\Module\\": "app/tests/modules/"
        }
    },
    "config": {
        "vendor-dir": "app/vendor"
    }
}
